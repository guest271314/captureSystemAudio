{
  "name": "Capture system audio",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [ 
    "nativeMessaging", 
    "tabs", 
    "activeTab", 
    "scripting", 
    "storage",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48":"icons-icons_dot_com_download_music_icon_123878_1.png"
  },
  "web_accessible_resources": [{
      "resources": [ "*.html", "*.js", "*.png" ],
      "matches": [ "<all_urls>" ],
      "extensions": [ ]
  }],
  "externally_connectable": {
    "matches": [],
    "ids": [
      "*"
    ]
  },
  "action": {
    "default_icon": {
      "16":"media-record-1-16x16.png", 
      "32":"media-record-1-32x32.png", 
      "48":"media-record-1-48x48.png", 
      "128":"media-record-1-128x128.png"
    }
  },
  "author": "guest271314",
  "homepage_url": "https://github.com/guest271314/captureSystemAudio/master/native_messaging/capture_system_audio/"
}
